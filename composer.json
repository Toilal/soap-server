{
  "name": "goetas-webservices/soap-server",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email": "goetas@gmail.com"
    }
  ],
  "require": {
    "php": "^5.5|^7.0",
    "goetas-webservices/soap-reader": "^0.3",
    "goetas-webservices/wsdl-reader": "^0.3",
    "goetas-webservices/xsd2php-runtime": "^0.2",
    "psr/http-message": "^1.0",
    "rybakit/arguments-resolver": "^0.5.0",
    "doctrine/instantiator": "^1.0.3",
    "psr/cache": "^1.0",
    "jms/serializer": "^1.2"
  },
  "require-dev": {
    "cache/doctrine-adapter": "^0.5",
    "zendframework/zend-diactoros": "^1.3",
    "phpunit/phpunit": "^4.8|^5.0",
    "goetas-webservices/xsd2php": "^0.1",
    "goetas-webservices/wsdl2php": "^0.1.2"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\Tests\\": "tests",
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "vendor/goetas-webservices/xsd2php/tests",
      "GoetasWebservices\\WsdlToPhp\\Tests\\": "vendor/goetas-webservices/wsdl2php/tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
