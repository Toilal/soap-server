	<wsdl:portType name="IstesBackoffice">

		<wsdl:operation name="login">

			<wsdl:documentation>Effettua il login, ritorna l'idsessione assegnato
				in questa sessione di login.
				Puo essere usato come cookieda invivare durante le richieste HTTP
				Il cookie dovrà chimarsi &quot;IstesSession&quot;</wsdl:documentation>

			<wsdl:input message="ist:loginRequest" />

			<wsdl:output message="ist:loginResponse" />

		</wsdl:operation>


		<wsdl:operation name="getEta">

			<wsdl:documentation>Ottiene tutte le fasce d'eta uttilizzabili
				dall'sistema

			</wsdl:documentation>

			<wsdl:input message="ist:getEtaRequest"></wsdl:input>

			<wsdl:output message="ist:getEtaResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="getTrattamenti">

			<wsdl:documentation>Ricevi i trattamenti (HB, BB, FB...)
				uttilizzabili dal'albergatore com il quale si è effettuato il login.

			</wsdl:documentation>

			<wsdl:input message="ist:getTrattamentiRequest"></wsdl:input>

			<wsdl:output message="ist:getTrattamentiResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="setOccupazione">

			<wsdl:documentation>Imposta sul server l'occupazione ed il minumum
				stay di alcune camere in alcuni giorni.

			</wsdl:documentation>

			<wsdl:input message="ist:setOccupazioneRequest"></wsdl:input>

			<wsdl:output message="ist:setOccupazioneResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="setCamera">

			<wsdl:documentation>Imposta una camera. Il codice camera deve essere
				univoco per albergo.

				Se si desidera, si possono anche impostare imedaitamente le regole di
				vendita per la camera
				(le regole devono essere state precedentemente create usando setRegola)


			</wsdl:documentation>

			<wsdl:input message="ist:setCameraRequest"></wsdl:input>

			<wsdl:output message="ist:setCameraResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="deleteCamera">

			<wsdl:documentation>Elimina una camera</wsdl:documentation>

			<wsdl:input message="ist:deleteCameraRequest"></wsdl:input>

			<wsdl:output message="ist:deleteCameraResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="setRegola">

			<wsdl:documentation>

				Crea una nuova regola con tanto di prezzi di
				vendita. Se

				la regola esiste già, verra solo aggiornata, in caso

				contrario verrà creata.

			</wsdl:documentation>

			<wsdl:input message="ist:setRegolaRequest"></wsdl:input>

			<wsdl:output message="ist:setRegolaResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="deleteRegola">

			<wsdl:documentation>Elimna una regola</wsdl:documentation>

			<wsdl:input message="ist:deleteRegolaRequest"></wsdl:input>

			<wsdl:output message="ist:deleteRegolaResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="associaCameraRegola">

			<wsdl:documentation>Associa una regola alla camera

			</wsdl:documentation>

			<wsdl:input message="ist:associaCameraRegolaRequest"></wsdl:input>

			<wsdl:output message="ist:associaCameraRegolaResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="rimuoviAssociazioniCameraRegola">

			<wsdl:documentation>Rimuove un associazione Camera-Regola





			</wsdl:documentation>

			<wsdl:input message="ist:rimuoviAssociazioniCameraRegolaRequest">

			</wsdl:input>

			<wsdl:output message="ist:rimuoviAssociazioniCameraRegolaResponse">

			</wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="setStagioni">

			<wsdl:documentation>Imposta le fascie stagionali con i relativi
				periodi. Le stagioni verrano usate per comporre il listino
			</wsdl:documentation>

			<wsdl:input message="ist:setStagioniRequest"></wsdl:input>

			<wsdl:output message="ist:setStagioniResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="setCamere">

			<wsdl:documentation>crea le camera</wsdl:documentation>

			<wsdl:input message="ist:setCamereRequest"></wsdl:input>

			<wsdl:output message="ist:setCamereResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="associaCameraRegolaRidotta">

			<wsdl:documentation>Associazione rapida di una regola con la relativa
				camera</wsdl:documentation>

			<wsdl:input message="ist:associaCameraRegolaRidottaRequest"></wsdl:input>

			<wsdl:output message="ist:associaCameraRegolaRidottaResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="rimuoviAssociazioniCamera">

			<wsdl:documentation>rimuovi un assocazione creata con
				&quot;associaCameraRegolaRidotta&quot; o con
				&quot;associaCameraRegola&quot; </wsdl:documentation>

			<wsdl:input message="ist:rimuoviAssociazioniCameraRequest"></wsdl:input>

			<wsdl:output message="ist:rimuoviAssociazioniCameraResponse"></wsdl:output>

		</wsdl:operation>

		<wsdl:operation name="setTipologie">
			<wsdl:documentation>Crea le tipologie di camere. Ogni camera dovrà
				essere associata ad una tipologia e anche ogni regola di vendità
				verra associata ad una tipologia.</wsdl:documentation>
			<wsdl:input message="ist:setTipologieRequest"></wsdl:input>

			<wsdl:output message="ist:setTipologieResponse"></wsdl:output>

		</wsdl:operation>

	</wsdl:portType>


